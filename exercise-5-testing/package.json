{
  "name": "exercise",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o",
    "serve:api": "json-server --watch db.json --delay 500 --port 4300",
    "build": "ng build --configuration production",
    "jest": "jest --config projects/todo-app/jest.config.js",
    "test": "npm run format:test && ng lint && npm run jest",
    "watch": "jest --watch --config projects/todo-app/jest.config.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier projects/**/*.{js,ts,json,html,scss,md} --write",
    "format:test": "prettier projects/**/*.{js,ts,json,html,scss,md} --list-different",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.14",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~12.2.14",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/router-store": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.14",
    "@angular/language-service": "~12.2.14",
    "@ngrx/schematics": "^12.5.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jest": "^26.6.3",
    "jest-createspyobj": "^1.2.2",
    "jest-preset-angular": "^8.3.2",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "typescript": "~4.3.5"
  }
}
