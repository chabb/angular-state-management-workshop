{
  "name": "exercise",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o",
    "serve:api": "json-server --watch db.json --delay 500 --port 4300",
    "build": "ng build --prod",
    "test": "npm run format:test && ng lint && ng test",
    "watch": "ng test --watchAll",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier projects/**/*.{js,ts,json,html,scss,md} --write",
    "format:test": "prettier projects/**/*.{js,ts,json,html,scss,md} --list-different"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "@ngrx/effects": "^8.5.2",
    "@ngrx/entity": "^8.5.2",
    "@ngrx/router-store": "^8.5.2",
    "@ngrx/store": "^8.5.2",
    "@ngrx/store-devtools": "^8.5.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.9.1"
