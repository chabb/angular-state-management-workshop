    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@ngrx/schematics": "^8.5.2",
    "@types/jest": "^24.0.18",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jest": "^24.9.0",
    "jest-createspyobj": "^1.2.2",
    "jest-preset-angular": "^7.1.1",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
